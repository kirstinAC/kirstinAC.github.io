<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.clientviews.pagecontrol.FacultyExternalBio" table="V_EXTERNAL_BIO">
        <id name="key" column="BIO_ID">
            <generator class="native"/>
        </id>
	    <property name="facId" column="FACULTY_ID"/>
	    <property name="displayName" column="DISPLAY_NM"/>
	    <property name="status" column="BIO_STATUS_CD"/>
	    <property name="desc" column="BIO_DESC"/>
	    <property name="webUrl" column="BIO_WEBSITE_LINK"/>
    </class>

   
</hibernate-mapping>