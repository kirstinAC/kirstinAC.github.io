<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.clientviews.pagecontrol.Faculty" table="V_FACULTY">
        <id name="key" column="FACULTY_ID">
            <generator class="native"/>
        </id>
	    <property name="firstName" column="FIRST_NM"/>
	    <property name="middleName" column="MIDDLE_NM"/>
	    <property name="lastName" column="LAST_NM"/>
	    <property name="nickName" column="NICK_NM"/>
	    <property name="email" column="EMAIL_ADDR"/>
	    <property name="emailWebAdvisor" column="EMAIL_ADDR_WEBADVISOR"/>
	    <property name="flagDirectory" column="DIRECTORY_FLAG"/>
	    <property name="flagValid" column="VALID_FACULTY_FLAG"/>
	    <property name="flagOverride" column="BIO_OVERRIDE_FLAG"/>
	    <property name="imageName" column="FAC_IMAGE_NM"/>
	    
	    
    </class>
    
</hibernate-mapping>
